<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault continueOnError="false" enabled="true" name="RaiseFault-AuthenticationError">
    <DisplayName>RaiseFault-AuthenticationError</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers/>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Unauthorized</ReasonPhrase>
            <Payload contentType="application/json">
{
    "code": "401",
    "message": "Unauthorized: client failed to provide valid credentials"
}
            </Payload>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>